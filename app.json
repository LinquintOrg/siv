{
  "expo": {
    "name": "Steam Inventory Value",
    "slug": "siv",
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/siv.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#091b53"
    },
    "jsEngine": "hermes",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/siv-foreground.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {},
      "package": "com.rsds.steaminventoryvalue",
      "versionCode": 2010000177
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "domr",
          "project": "domr"
        }
      ],
      "expo-font",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "9ee78a6c-9709-416e-baf5-905247436ac4"
      }
    },
    "scheme": "siv-scheme"
  }
}